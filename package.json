{
  "name": "notes-client-dotnet",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "generate-live:client && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "lint": "eslint",
    "format": "prettier",
    "check": "prettier --write . && eslint --fix",
    "update:api": "rm -rf api && npm run generate:client",
    "generate:client": "openapi-generator-cli generate -i http://localhost:5004/openapi/v1.json -g typescript-fetch -o api",
    "generate-live:client": "openapi-generator-cli generate -i https://notesapi-h9hcafhychchhnb5.australiasoutheast-01.azurewebsites.net/openapi/v1.json -g typescript-fetch -o api --skip-validate-spec"
  },
  "dependencies": {
    "@lexical/react": "^0.39.0",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-devtools": "^0.7.0",
    "@tanstack/react-form": "^1.0.0",
    "@tanstack/react-query": "^5.66.5",
    "@tanstack/react-query-devtools": "^5.84.2",
    "@tanstack/react-router": "^1.132.0",
    "@tanstack/react-router-devtools": "^1.132.0",
    "@tanstack/react-virtual": "^3.13.18",
    "@tanstack/router-plugin": "^1.132.0",
    "bootstrap": "^5.3.8",
    "bootswatch": "^5.3.8",
    "classnames": "^2.5.1",
    "lexical": "^0.39.0",
    "lucide-react": "^0.545.0",
    "react": "^19.2.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.0.6",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.27.0",
    "@tanstack/devtools-vite": "^0.3.11",
    "@tanstack/eslint-config": "^0.3.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "jsdom": "^27.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.7.2",
    "vite": "^7.1.7",
    "vitest": "^3.0.5",
    "web-vitals": "^5.1.0"
  }
}
